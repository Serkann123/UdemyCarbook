@{
    ViewData["Title"] = "Araba Detay";
}

<section class="ftco-section ftco-car-details">
	<div class="container">

		@await Component.InvokeAsync("_CarDetailMainCarFeatureComponentPartial",new {id=ViewBag.carId})

		<div class="row">
			<div class="col-md-12 pills">
				<div class="bd-example bd-example-tabs">
				
					@await Component.InvokeAsync("_CarDetailTabPanelComponentPartial")

					<div class="tab-content" id="pills-tabContent">

						@await Component.InvokeAsync("_CarDetailCarFeatureByCarIdComponentPartial", new { id = ViewBag.carId })

						@await Component.InvokeAsync("_CarDescriptionByCarIdComponenetPartial", new { id = ViewBag.carId })

						@await Component.InvokeAsync("_CarDetailCommentCarByIdComponentPartial",new {id=ViewBag.carId})
					</div>
				</div>
			</div>
		</div>
	</div>
</section>

