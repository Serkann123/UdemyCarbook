
@{
    ViewData["Title"] = "BlogDetail";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<section class="ftco-section ftco-degree-bg">
    <div class="container">
        <div class="row">
       
            @await Component.InvokeAsync("_BlogDetailsMainCompenentPartial", new { id = ViewBag.blogİd })
            <div class="col-md-4 sidebar ftco-animate">

                @await Component.InvokeAsync("_BlogDetailSidebarSearchBoxComponentPartial")

                @await Component.InvokeAsync("_BlogDetailsCategoryViewComponentsPartial")

                @await Component.InvokeAsync("_BlogDetailRecentBlogComponentPartial")

                @await Component.InvokeAsync("_BlogDetailTagCloudComponentPartial",new {id=ViewBag.blogİd})

                @await Component.InvokeAsync("_BlogDetailParagraphComponentPartial")
            </div>

        </div>
    </div>
</section>

