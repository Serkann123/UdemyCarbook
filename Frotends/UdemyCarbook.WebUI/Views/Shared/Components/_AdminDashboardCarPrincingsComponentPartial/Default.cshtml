@model List<ResultCarPrincingListModelDto>

<div class="row">
	<div class="col-xl-6">
		<div class="card">
			<div class="card-body">

				<h4 class="card-title">Araç Kiralana Ücretleri</h4>
				<p class="card-subtitle mb-4 font-size-13">
					Bazı Araçların günlük-haftalık-aylık kiralama fiyatları
				</p>

				<div class="table-responsive">
					<table class="table table-centered table-hover table-xl mb-0" id="recent-orders">
